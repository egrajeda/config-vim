" Apariencia
set guioptions=aegiLt

set guifont=Inconsolata\ 10

let g:molokai_original = 1
colorscheme molokai

syntax on

hi NonText    guifg=#373832 guibg=bg
hi SpecialKey guifg=#676862 guibg=bg

set listchars=eol:¶,trail:•
set list

set nowrap
set autoindent

" Estandares
set shiftwidth=4
set tabstop=4
set expandtab

" Keybindings
nm , :
